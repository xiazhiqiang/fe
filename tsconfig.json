{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "module": "commonjs",
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
